{
  "extends": ["config:base"],
  "dependencyDashboard": true,
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["^@docusaurus"],
      "allowedVersions": "/^\\d+\\.\\d+\\.\\d+(-alpha\\.(\\d+))?$/"
    }
  ],
  "supportPolicy": ["lts_latest"]
}
